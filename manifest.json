{
    "manifest_version": 3,
    "name": "NHKPP",
    "version": "1.0",
    "description": "Disables enterpictureinpicture and leavepictureinpicture events on NHK Plus.",
    "content_scripts": [
        {
            "matches": [
                "https://plus.nhk.jp/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}